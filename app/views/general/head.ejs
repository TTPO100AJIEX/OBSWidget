<% const style = locals.style ?? "light"; %>

<% if (style == "dark")
{ %>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Inconsolata:wght@200..900&display=swap" nonce="<%=cspNonce.style%>">
<% } %>

<link rel="stylesheet" href="/static/css/general/common.css" nonce="<%=cspNonce.style%>">
<link rel="stylesheet" href="/static/css/general/<%=style%>.css" nonce="<%=cspNonce.style%>">
<script src="/static/js/general.js" nonce="<%=cspNonce.script%>" type="module"></script>

<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">

<link rel="canonical" href="https://<%=config.website.host%><%=req.routeOptions.url%>">

<link rel="icon" type="image/svg+xml" href="/static/images/favicon/favicon.svg" sizes="any">
<link rel="icon" type="image/x-icon" href="/static/images/favicon/favicon.ico" sizes="32x32">
<link rel="icon" type="image/png" href="/static/images/favicon/favicon_48x48.png" sizes="48x48">
<link rel="icon" type="image/png" href="/static/images/favicon/favicon_96x96.png" sizes="96x96">
<link rel="icon" type="image/png" href="/static/images/favicon/favicon_144x144.png" sizes="144x144">
<link rel="icon" type="image/png" href="/static/images/favicon/favicon_192x192.png" sizes="192x192">
<link rel="apple-touch-icon" type="image/png" href="/static/images/favicon/apple-touch-icon.png" sizes="180x180">

<meta name="theme-color" content="<%=style == 'light' ? 'hsl(223, 30%, 95%)' : 'hsl(264, 57%, 13%)'%>">
<meta name="supported-color-schemes" content="<%=style%>">